{
  "name": "apillon-sdk-sandbox",
  "version": "1.0.0",
  "description": "Apillon SDK usage example.",
  "main": "dist/index",
  "scripts": {
    "start": "nodemon --exec npx ts-node src/index.ts",
    "identity": "npx ts-node ./src/identity.ts",
    "identity:generate-message": "npx ts-node ./src/identity-generate-message.ts",
    "identity:validate-signature": "npx ts-node ./src/identity-validate-signature.ts",
    "storage:buckets": "npx ts-node ./src/storage-buckets.ts",
    "storage:bucket-content": "npx ts-node ./src/storage-bucket-content.ts",
    "storage:bucket-upload": "npx ts-node ./src/storage-bucket-upload.ts",
    "storage:bucket-upload-from-folder": "npx ts-node ./src/storage-bucket-upload-from-folder.ts",
    "storage:bucket-ipns-create": "npx ts-node ./src/storage-bucket-ipns-create.ts",
    "storage:bucket-ipns-publish": "npx ts-node ./src/storage-bucket-ipns-publish.ts",
    "hosting:websites": "npx ts-node ./src/hosting-websites.ts",
    "hosting:website": "npx ts-node ./src/hosting-website.ts",
    "hosting:website-deploy": "npx ts-node ./src/hosting-website-deploy.ts",
    "hosting:website-deploy-status": "npx ts-node ./src/hosting-website-deploy-status.ts",
    "nft:collections": "npx ts-node ./src/nft-collections.ts",
    "nft:collection": "npx ts-node ./src/nft-collection.ts",
    "nft:collection-create": "npx ts-node ./src/nft-collection-create.ts",
    "nft:collection-mint": "npx ts-node ./src/nft-collection-mint.ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "@apillon/sdk": "^1.2.0-beta.1",
    "dotenv": "^16.3.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
